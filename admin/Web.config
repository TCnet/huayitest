<?xml version="1.0" encoding="utf-8"?>
<!-- 注意: 除了手动编辑此文件以外，您还可以使用 Web 管理工具来 
     配置应用程序的设置。可以使用 Visual Studio 中的“网站”->“ASP.NET 配置”
     选项。
     设置和注释的完整列表在
     machine.config.comments 中，该文件通常位于 \Windows\Microsoft.NET\Framework\v2.0.xxxxx\Config 中
 -->
<configuration>
  <appSettings>
    <add key="AdminTitle" value="华一检测管理后台" />
    <add key="ImgPath" value="/attachment/Image/" />
    <add key="FilePath" value="/attachment/File/" />
  </appSettings>
  <location path="Default.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="nologin.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
 
  <location path="images">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  
  <!--指定对整个Admin目录的访问权限-->
  <system.web>
    <authorization>
      <!--多个角色用,分隔-->
      <allow roles="admin"/>
      <deny users="*" />
    </authorization>
  </system.web> 

</configuration>
